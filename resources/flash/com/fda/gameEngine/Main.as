package com.fda.gameEngine{	import flash.display.*;	import flash.events.Event;	import flash.events.KeyboardEvent;	import flash.ui.Keyboard;	import flash.media.Sound; 		public class Main extends MovieClip	{		private static var keyCheck:Boolean;		private var _bullet:Bullet;		private var _sfx_shoot:SFXShoot = new SFXShoot();				public function Main()		{			stage.addEventListener(KeyboardEvent.KEY_DOWN, actionKeyDown);			addEventListener(Event.ENTER_FRAME, onFrame);		}				private function actionKeyDown(event:KeyboardEvent):void		{			switch(event.keyCode)			{				case Keyboard.SPACE:				_bullet = new Bullet();				_bullet.x = hero.x;				_bullet.y = hero.y;				addChild(_bullet);				_bullet.fireBullet();				_sfx_shoot.play();			}		}				private function onFrame(event:Event):void		{			hero.update();			enemy1.update();			enemy2.update();			enemy3.update();		}	}}